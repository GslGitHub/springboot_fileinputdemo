<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.filedemo.dao.ChanpinDAO">    <resultMap id="BaseResultMap" type="com.filedemo.dto.ChanpinDTO">        <result column="id" jdbcType="INTEGER" property="id" />        <result column="chanpin_name" jdbcType="VARCHAR" property="chanpinName" />        <result column="file_path" jdbcType="VARCHAR" property="filePath" />        <result column="file_name" jdbcType="VARCHAR" property="fileName" />        <result column="bus_id" jdbcType="INTEGER" property="busId" />        <result column="scrq" jdbcType="TIMESTAMP" property="scrq" />    </resultMap>    <select id="list" resultMap="BaseResultMap">        select * from chanpin order by scrq desc    </select>    <delete id="delt">        delete from chanpin where id = #{id}    </delete>    <select id="getFileCount" resultType="java.lang.Integer">        select count(*) from chanpin where bus_id = #{id}    </select>    <insert id="create">        insert into chanpin (chanpin_name,bus_id,file_name,file_path) value (#{chanpinName},#{busId},#{fileName},#{filePath})    </insert></mapper>